{
  "name": "BagelsShop",
  "version": "0.1.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build --mod",
    "lint": "vue-cli-service lint",
    "test:unit": "vue-cli-service test:unit",
    "production": "cd frontend/dist && google-chrome-stable http://localhost:8000 && python2 -m SimpleHTTPServer 8000",
    "fake": "json-server --watch fake.json --routes fake.routes.json"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.0.0-beta.2",
    "bootstrap-vue": "^2.0.0-rc.11",
    "jquery": "^3.2.1",
    "vue": "^2.5.16",
    "vue-i18n": "^8.0.0",
    "vue-router": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-eslint": "^3.0.0-rc.9",
    "@vue/cli-plugin-unit-mocha": "^3.0.0-rc.9",
    "@vue/cli-service": "^3.0.0-rc.9",
    "@vue/eslint-config-standard": "^3.0.0-rc.9",
    "@vue/test-utils": "^1.0.0-beta.22",
    "chai": "^4.1.2",
    "vue-template-compiler": "^2.5.16"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "@vue/standard"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
